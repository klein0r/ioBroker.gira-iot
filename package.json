{
  "name": "iobroker.gira-iot",
  "version": "0.0.1",
  "description": "Integrate your Gira X1 or HomeServer",
  "author": {
    "name": "Matthias Kleine",
    "email": "info@haus-automatisierung.com",
    "url": "https://haus-automatisierung.com/"
  },
  "homepage": "https://github.com/klein0r/ioBroker.gira-iot",
  "license": "MIT",
  "keywords": [
    "ioBroker",
    "Smart Home",
    "home automation",
    "gira",
    "x1",
    "homeserver",
    "iot"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/klein0r/ioBroker.gira-iot.git"
  },
  "engines": {
    "node": ">=12.0.0"
  },
  "dependencies": {
    "@iobroker/adapter-core": "^2.6.0"
  },
  "devDependencies": {
    "@alcalzone/release-script": "^2.2.2",
    "@iobroker/adapter-dev": "^1.0.0",
    "@iobroker/adapter-react": "2.0.22",
    "@iobroker/testing": "^2.6.0",
    "@material-ui/core": "^4.12.4",
    "@types/chai": "^4.3.1",
    "@types/chai-as-promised": "^7.1.5",
    "@types/mocha": "^9.1.1",
    "@types/node": "^14.18.16",
    "@types/proxyquire": "^1.3.28",
    "@types/react-dom": "^17.0.16",
    "@types/react": "^17.0.44",
    "@types/sinon": "^10.0.11",
    "@types/sinon-chai": "^3.2.8",
    "chai": "^4.3.6",
    "chai-as-promised": "^7.1.1",
    "eslint": "^8.14.0",
    "eslint-plugin-react": "^7.29.4",
    "mocha": "^9.2.2",
    "proxyquire": "^2.1.3",
    "react-dom": "^17.0.2",
    "react": "^17.0.2",
    "sinon": "^13.0.2",
    "sinon-chai": "^3.7.0",
    "typescript": "~4.5.5"
  },
  "main": "main.js",
  "files": [
    "admin{,/!(src)/**}/!(tsconfig|tsconfig.*).json",
    "admin{,/!(src)/**}/*.{html,css,png,svg,jpg,js}",
    "admin/build/",
    "lib/",
    "www/",
    "io-package.json",
    "LICENSE",
    "main.js"
  ],
  "scripts": {
    "prebuild": "rimraf admin/build",
    "build": "build-adapter react",
    "watch": "build-adapter react --watch",
    "prebuild:react": "rimraf admin/build",
    "build:react": "build-adapter react",
    "watch:react": "build-adapter react --watch",
    "test:js": "mocha --config test/mocharc.custom.json \"{!(node_modules|test)/**/*.test.js,*.test.js,test/**/test!(PackageFiles|Startup).js}\"",
    "test:package": "mocha test/package --exit",
    "test:unit": "mocha test/unit --exit",
    "test:integration": "mocha test/integration --exit",
    "test": "npm run test:js && npm run test:package",
    "check": "tsc --noEmit -p tsconfig.check.json",
    "lint": "eslint --ext .js,.jsx",
    "translate": "translate-adapter",
    "release": "release-script"
  },
  "bugs": {
    "url": "https://github.com/klein0r/ioBroker.gira-iot/issues"
  },
  "readmeFilename": "README.md"
}